/*2 CATEGORY*/
DATA B79.C;SET SASHELP.CLASS;FORMAT X 10.2;
IF SEX = "F" THEN X = LOG(AGE);ELSE X = AGE**2;RUN;
/*> 2 CATEGORIES*/
DATA B79.C;SET SASHELP.CLASS;FORMAT TAG $30.;
IF AGE < 13 THEN TAG = "AGE BELOW 13";
ELSE IF AGE <=14 THEN TAG = "AGE BETWEEN 13 AND 14";
ELSE TAG = "AGE ABOVE 14";RUN;
/*MULTIPLE ACTION*/
DATA NEW;
SET OLD;
IF CONDITION THEN DO;
ACTION1;ACTION2;...;ACTION N;END;
ELSE IF CONDITION THEN DO;
ACTION1;ACTION2;...;ACTION N;END;
.....;
ELSE DO;
ACTION1;ACTION2;...;ACTION N;END;RUN;
DATA B79.C;SET SASHELP.CLASS;FORMAT TAG $30.;
FORMAT X Y Z 10.2;
IF AGE < 13 THEN DO;
X = LOG(AGE);Y = LOG(HEIGHT);
Z=LOG(WEIGHT);TAG = "AGE BELOW 13";END;
ELSE IF AGE <=14 THEN DO;
X = 1000;Y=999;Z = 888;
TAG = "AGE BETWEEN 13 AND 14";END;
ELSE DO;X = AGE**2;Y=HEIGHT**2;
Z = WEIGHT**2;TAG = "AGE ABOVE 14";END;RUN;
