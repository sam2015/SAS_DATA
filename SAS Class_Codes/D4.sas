/*PRINT*/
/*ENTIRE DATA*/
PROC PRINT DATA = SASHELP.CLASS;RUN;
/*SOME VARIABLES*/
PROC PRINT DATA = SASHELP.CLASS;VAR NAME AGE SEX;RUN;
/*SOME OBS*/
PROC PRINT DATA = SASHELP.CLASS (OBS = 6);
VAR NAME AGE SEX;RUN;
PROC PRINT DATA = SASHELP.CLASS (FIRSTOBS = 3 OBS = 8);
VAR NAME AGE SEX;RUN;
/*CONDITIONS*/
PROC PRINT DATA = SASHELP.CLASS;VAR NAME AGE SEX;
WHERE SEX = "F";RUN;
PROC PRINT DATA = SASHELP.CLASS;VAR NAME AGE SEX;
WHERE AGE<13;RUN;
PROC PRINT DATA = SASHELP.CLASS;VAR NAME AGE SEX;
WHERE AGE>14;RUN;
PROC PRINT DATA = SASHELP.CLASS;VAR NAME AGE SEX;
WHERE 13<=AGE<=14;RUN;
PROC PRINT DATA = SASHELP.CLASS;VAR NAME AGE SEX;
WHERE AGE<13 OR AGE > 14;RUN;
PROC PRINT DATA = SASHELP.CLASS;VAR NAME AGE SEX;
WHERE NOT(13<=AGE<=14);RUN;
PROC PRINT DATA = SASHELP.CLASS;VAR NAME AGE SEX;
WHERE SEX = "F" AND NOT(13<=AGE<=14);RUN;
/*PRINT ALL DATES THAT ARE WEEKENDS AFTER 1955*/
PROC PRINT DATA = SASHELP.AIR;VAR DATE;
FORMAT DATE WEEKDATE30.;
WHERE YEAR(DATE) > 1955 AND 
(WEEKDAY(DATE) = 1 OR WEEKDAY(DATE) = 7);RUN;
PROC PRINT DATA = SASHELP.AIR;VAR DATE;
FORMAT DATE WEEKDATE30.;
WHERE YEAR(DATE) > 1955 AND 
WEEKDAY(DATE) IN (1,7);RUN;
PROC PRINT DATA = SASHELP.AIR;VAR DATE;
FORMAT DATE WEEKDATE30.;
WHERE YEAR(DATE) > 1955 AND 
NOT(2<=WEEKDAY(DATE) <=6);RUN;
/*PRINT NAME SEX AGE OF FEMALES WITH NAME STARTING WITH*/
/*J*/
PROC PRINT DATA = SASHELP.CLASS;VAR NAME SEX AGE;
WHERE SEX = "F" AND SUBSTR(NAME,1,1) = "J";RUN;
PROC PRINT DATA = SASHELP.CLASS;VAR NAME SEX AGE;
WHERE SEX = "F" AND INDEX(NAME,"J") = 1;RUN;
PROC PRINT DATA = SASHELP.CLASS;VAR NAME SEX AGE;
WHERE SEX = "F" AND NAME LIKE "J%";RUN;
